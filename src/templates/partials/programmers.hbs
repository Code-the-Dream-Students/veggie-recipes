      {{#each programmers  as | programmer |}}
        <div class="b col-md-6 col-lg-4 mb-5 p-3">
        <div class="card border rounded">
          <img src={{image}} class="mt-4 w-75 mx-auto border rounded-circle" alt="">
          <!-- <div style="background-image: url('./images/broc-pasta.jpg')" class="card-img-top"></div> -->
          <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <p class="card-text">
              {{description}}
            </p>
            <div class="d-flex justify-content-between">
              <button class="btn bg-primary mr-2 w-50" type="submit" data-toggle="modal" data-target="#recipeModal">
                <i class="fa fa-linkedin" aria-hidden="true"></i>
                Linkedin
              </button>
              <button class="btn bg-success ml-2 w-50" type="submit" data-toggle="modal" data-target="#recipeModal">
                <i class="fab fa-github"></i>
                GitHub
              </button>
            </div>
          </div>
        </div>
      </div>
      {{/each}}